@model IEnumerable<CapStore.Models.Customer>

    <div class="mainmenu-area">
        <div class="container">
            <div class="row">
                <h2>List of Users</h2>
                <table class="table">
                    @foreach (var user in Model)
                    {
                        <tr>
                            <td>@user.Email</td>
                            <td>@user.UserName</td>
                            <td>
                                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Access rights</a>
                            </td>
                        </tr>
                    }
                </table>
                </div>
</div>
</div>